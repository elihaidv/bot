{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Main",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "ts-node",
            "args": [
                "Main.ts"
            ],
            "cwd": "${workspaceRoot}",
            "internalConsoleOptions": "openOnSessionStart",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**"
            ]
        },
        {
            "name": "Channel",
            "program": "${workspaceFolder}/Simulator/SimulateChannel.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Simulate",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "ts-node",
            "args": [
                "Simulator/Simulate.ts",
                "7",
                "634fc5e6c32f3542e2200e33",
                "2022-09-04 13:46:52",
                "2022-10-05 20:07:46",
                "quiet"
            ],
            "cwd": "${workspaceRoot}",
            "internalConsoleOptions": "openOnSessionStart",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**"
            ]
        },
        {
            "name": "fetch history",
            "program": "${workspaceFolder}/Simulator/historicalChartsFetcher.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "MATICBUSD", "2021-08-28 15:32:19"
            ],
            "type": "node"
        },
        {
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}",
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        }
    ]
}