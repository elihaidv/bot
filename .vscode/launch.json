{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Main",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "ts-node",
            "args": [
                "Main.ts"
            ],
            "cwd": "${workspaceRoot}",
            "internalConsoleOptions": "openOnSessionStart",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**"
            ]
        },
        {
            "name": "Channel",
            "program": "${workspaceFolder}/Simulator/SimulateChannel.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Simulate",
            "type": "node",
            "request": "launch",
            "runtimeExecutable": "ts-node",
            "args": [
                "Simulator/local.ts",
                "63b2e68b4283983ce67aa192",
                "0-8",
                "2022-01-02 14:02:51",
                "2023-01-02 14:02:51",
                "quiet"
            ],
            "cwd": "${workspaceRoot}",
            "internalConsoleOptions": "openOnSessionStart",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**"
            ],
            "env": {
                "GOOGLE_APPLICATION_CREDENTIALS": "trading-cloud.json",
                "TZ":"UTC"
            }
        },
        {
            "name": "fetch history",
            "program": "${workspaceFolder}/Simulator/historicalChartsFetcher.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "MATICBUSD", "2021-08-28 15:32:19"
            ],
            "type": "node"
        },
        {
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}",
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        }
    ]
}